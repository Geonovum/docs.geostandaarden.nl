<definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string" xml:lang="nl">Buis voor bescherming van kabels, buizen en HDPE buizen.</definition>
<hasTopConcept rdf:resource="https://definities.geostandaarden.nl/imgeo/doc/begrip/Leiding"/>
<inScheme rdf:resource="http://definities.geostandaarden.nl/imgeo/geobor/imbor/2020#TypeLeidingBOR"/>
</Concept>
<Concept xmlns="http://www.w3.org/2004/02/skos/core#" rdf:about="http://definities.geostandaarden.nl/imgeo/geobor/imbor/2020#TypeLeidingBOR_ontluchtingsleiding">
<type xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
